echo "ADDCAR TEAM: BB, CAR: 21, SPEED: 4000, CONSUMPTION: 5.5, RELIABILITY: 80" > $PIPE_NAME
echo "ADDCAR TEAM: BB, CAR: 22, SPEED: 3800, CONSUMPTION: 3.8, RELIABILITY: 80" > $PIPE_NAME
echo "START RACE!" > $PIPE_NAME
echo "ADDCAR TEAMS: CC" > $PIPE_NAME


